{
	"name": "iframe-wallet",
	"version": "0.0.9",
	"main": "dist",
	"scripts": {
		"dev": "svelte-kit dev --port=3444",
		"build": "node fix-new-urls.js && svelte-kit build",
		"preview": "svelte-kit preview --port=3445",
		"package": "node fix-new-urls.js && svelte-kit package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "mocha",
		"deploy": "node --experimental-specifier-resolution=node --trace-warnings arlog-deploy.js ",
		"arlog:deploy": "npm run build && node --experimental-specifier-resolution=node --trace-warnings deploy.js ",
		"ipfs:deploy": "npm run build && node --experimental-specifier-resolution=node --trace-warnings ./ipfs-deploy.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"base64-js": "^1.5.1",
		"cli-color": "^0.3.2",
		"clui": "^0.3.6",
		"conf": "^10.1.1",
		"cross-spawn": "^7.0.3",
		"fs": "^0.0.1-security",
		"ipfs-http-client": "^52.0.1",
		"micromatch": "^4.0.4",
		"path": "^0.12.7",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte2tsx": "^0.4.8",
		"sveltejs-adapter-ipfs": "^0.1.6",
		"@sveltejs/adapter-static": "^1.0.0-next.24",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"url": "^0.11.0"
	},
	"type": "module",
	"dependencies": {
		"buffer": "^6.0.3",
		"immortal-db": "^1.1.0",
		"bignumber.js": "^9.0.2"
	},
	"license": "MIT+CC",
	"engines": {
		"npm": ">=8.0.0",
		"node": ">=14.18.1"
	}
}
